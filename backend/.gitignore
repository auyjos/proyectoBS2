# .gitignore for Node.js projects

# Environment variables
.env
.env.local
.env.*.local

# Node.js
node_modules/

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Dependency directories
/lib-cov/
/lib/cache/
/lib/pipette/
*.hbs.cache/
/node_modules/
/pnpm-lock.yaml
/test/tmp/
/test/fixtures/
/test/node_modules/
/.pnp/
.pnp.js

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity
